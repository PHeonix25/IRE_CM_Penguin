{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Enable IAM User Permissions",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::145563897443:root"
            },
            "NotAction": [
                "kms:ScheduleKeyDeletion",
                "kms:DeleteImportedKeyMaterial"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Allow decrypt using the key",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::145563897443:role/AWSManagedServicesAdminRole",
                    "arn:aws:iam::145563897443:role/AWSManagedServicesChangeManagementRole",
                    "arn:aws:iam::145563897443:role/IRE-CM-Penguin-DEV-API-Role",
                    "arn:aws:iam::145563897443:role/aws-service-role/autoscaling.amazonaws.com/AWSServiceRoleForAutoScaling"
                ]
            },
            "Action": [
                "kms:DescribeKey",
                "kms:Decrypt"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Allow encrypt using the key",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::145563897443:role/AWSManagedServicesAdminRole",
                    "arn:aws:iam::145563897443:role/AWSManagedServicesChangeManagementRole",
                    "arn:aws:iam::145563897443:role/IRE-CM-Penguin-DEV-API-Role",
                    "arn:aws:iam::145563897443:role/aws-service-role/autoscaling.amazonaws.com/AWSServiceRoleForAutoScaling"
                ]
            },
            "Action": [
                "kms:DescribeKey",
                "kms:Encrypt",
                "kms:ReEncrypt*",
                "kms:GenerateDataKey",
                "kms:GenerateDataKeyWithoutPlaintext"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Allow grants using the key",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::145563897443:role/AWSManagedServicesAdminRole",
                    "arn:aws:iam::145563897443:role/AWSManagedServicesChangeManagementRole",
                    "arn:aws:iam::145563897443:role/IRE-CM-Penguin-DEV-API-Role",
                    "arn:aws:iam::145563897443:role/aws-service-role/autoscaling.amazonaws.com/AWSServiceRoleForAutoScaling"
                ]
            },
            "Action": [
                "kms:CreateGrant",
                "kms:ListGrants",
                "kms:RevokeGrant"
              ],
            "Resource": "*"
        },
        {
            "Sid": "Allow service-linked role use of the CMK",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::145563897443:role/aws-service-role/autoscaling.amazonaws.com/AWSServiceRoleForAutoScaling"
            },
            "Action": "kms:GenerateDataKey*",
            "Resource": "*"
        },
        {
            "Sid": "Allow attachment of persistent resources",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::145563897443:role/aws-service-role/autoscaling.amazonaws.com/AWSServiceRoleForAutoScaling"
            },
            "Action": "kms:CreateGrant",
            "Resource": "*",
            "Condition": {
                "Bool": {
                    "kms:GrantIsForAWSResource": "true"
                }
            }
        }
    ]
}